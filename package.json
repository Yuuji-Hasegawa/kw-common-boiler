{
  "name": "kw-common-boiler",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "simple-git-hooks",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"src/**/*.js\"",
    "lint:prettier": "prettier --check src",
    "lint:stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "fix": "run-p fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "prettier --write src",
    "fix:stylelint": "npm run lint:stylelint -- --fix",
    "image": "run-s image:clean image:opt image:gen",
    "image:opt": "node npm-scripts/opt.mjs",
    "image:gen": "node npm-scripts/gen.mjs",
    "image:clean": "rimraf public/img",
    "icon": "run-s icon:clean icon:gen",
    "icon:gen": "node npm-scripts/icon.mjs",
    "icon:clean": "rimraf public/icons/icon-*.png"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix --quiet"
    ],
    "**/*.{css,scss}": [
      "stylelint --fix --quiet"
    ],
    "**/*.{html,json,jsonc}": [
      "prettier --write --loglevel=error"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpx lint-staged"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.2.0",
    "@storybook/addon-interactions": "^7.2.0",
    "@storybook/addon-links": "^7.2.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-styling": "^1.3.5",
    "@storybook/blocks": "^7.2.0",
    "@storybook/react": "^7.2.0",
    "@storybook/react-vite": "^7.2.0",
    "@storybook/testing-library": "^0.2.0",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.14",
    "css-declaration-sorter": "^7.0.3",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-css-import-order": "^1.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-unused-imports": "^3.0.0",
    "fs": "0.0.1-security",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^8.4.27",
    "postcss-preset-env": "^9.1.0",
    "prettier": "^3.0.0",
    "prop-types": "^15.8.1",
    "rimraf": "^5.0.1",
    "sass": "^1.64.2",
    "sharp": "^0.32.4",
    "simple-git-hooks": "^2.9.0",
    "storybook": "^7.2.0",
    "storybook-addon-swc": "^1.2.0",
    "stylelint": "^15.10.2",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "terser": "^5.19.2",
    "vite": "^4.4.8"
  }
}
